{
  "name": "@lexical/accessibility",
  "description": "Accessibility plugin for Lexical - Screen reader announcements for editor operations.",
  "keywords": [
    "lexical",
    "accessibility",
    "a11y",
    "screen-reader",
    "aria",
    "wcag",
    "nvda",
    "jaws",
    "voiceover"
  ],
  "license": "MIT",
  "version": "0.38.2",
  "main": "LexicalAccessibility.js",
  "types": "index.d.ts",
  "module": "LexicalAccessibility.mjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/lexical",
    "directory": "packages/lexical-accessibility"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "development": "./LexicalAccessibility.dev.mjs",
        "production": "./LexicalAccessibility.prod.mjs",
        "node": "./LexicalAccessibility.node.mjs",
        "default": "./LexicalAccessibility.mjs"
      },
      "require": {
        "types": "./index.d.ts",
        "development": "./LexicalAccessibility.dev.js",
        "production": "./LexicalAccessibility.prod.js",
        "default": "./LexicalAccessibility.js"
      }
    }
  },
  "dependencies": {
    "@lexical/code": "workspace:*",
    "@lexical/link": "workspace:*",
    "@lexical/list": "workspace:*",
    "@lexical/markdown": "workspace:*",
    "@lexical/react": "workspace:*",
    "@lexical/rich-text": "workspace:*",
    "lexical": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
